{
  "expo": {
    "name": "Socratic Coach Mobile",
    "slug": "socratic-coach",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./mobile/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./mobile/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "mobile/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.socraticcoach.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./mobile/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.socraticcoach.app"
    },
    "web": {
      "favicon": "./mobile/assets/favicon.png",
      "bundler": "metro"
    },
    "scheme": "socratic-coach",
    "experiments": {
      "typedRoutes": true
    },
    "platforms": ["ios", "android", "web"]
  }
}